<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<ScrollPane maxHeight="705.0" maxWidth="1360.0" minHeight="705.0" minWidth="1360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ProductController">
   <content>
      <VBox maxWidth="1350.0" minWidth="1350.0" prefWidth="1350.0" spacing="8.0">
         <children>
            <HBox spacing="8.0">
               <children>
                  <Button fx:id="back" mnemonicParsing="false" onAction="#back" text="Back" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="cart" mnemonicParsing="false" text="Cart" />
                  <Button fx:id="login" mnemonicParsing="false" text="Login" />
                  <Button fx:id="account" mnemonicParsing="false" text="Account" />
                  <Button fx:id="logout" mnemonicParsing="false" prefHeight="25.0" prefWidth="69.0" text="Logout" />
               </children></HBox>
            <HBox prefHeight="221.0" prefWidth="1360.0" spacing="8.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="258.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
                  <VBox alignment="CENTER" prefHeight="205.0" prefWidth="189.0" spacing="8.0">
                     <children>
                        <Label fx:id="name" text="Label" textAlignment="CENTER" />
                        <Text fx:id="info" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" />
                        <Label fx:id="company" text="Label" textAlignment="CENTER" />
                        <Label fx:id="numberseen" text="Label" textAlignment="CENTER" />
                        <HBox fx:id="rate" prefHeight="100.0" prefWidth="200.0" />
                     </children>
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </VBox>
                  <Region prefHeight="258.0" prefWidth="261.0" />
                  <ScrollPane fx:id="fieldsOfCategory" prefHeight="242.0" prefWidth="315.0">
                     <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </HBox.margin>
                  </ScrollPane>
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding></HBox>
            <HBox maxHeight="350.0" prefHeight="400.0" prefWidth="1360.0" spacing="8.0">
               <children>
                  <ScrollPane fx:id="productFields" prefHeight="384.0" prefWidth="261.0" />
                  <Region prefHeight="334.0" prefWidth="105.0" />
                  <ScrollPane fx:id="comments" prefHeight="334.0" prefWidth="906.0" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding></HBox>
            <HBox prefHeight="30.0" prefWidth="1360.0">
               <children>
                  <Button fx:id="addToCart" mnemonicParsing="false" text="Add To Cart" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </content>
</ScrollPane>
