<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>


<HBox maxWidth="400.0" minWidth="-Infinity" prefWidth="400.0" spacing="8.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ProductFieldController">
   <children>
      <Label fx:id="companyName" text="Label" />
      <Region HBox.hgrow="ALWAYS" />
      <Label fx:id="priceAfterSale" />
      <Label fx:id="price" text="Label" />
      <Button mnemonicParsing="false" text="Add To Cart" />
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</HBox>
