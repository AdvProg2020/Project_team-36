<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="705.0" prefWidth="1360.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ManagerRegisterController">
<children>
    <VBox alignment="CENTER" layoutX="742.0" layoutY="57.0" prefHeight="576.0" prefWidth="279.0" spacing="20.0">
        <children>
            <TextField fx:id="username" promptText="username" />
